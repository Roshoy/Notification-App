<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0"
            fx:controller="Controller.CoordinatorController">

    <TableView prefHeight="600.0" prefWidth="600.0" BorderPane.alignment="CENTER">
        <columns>
            <TableColumn prefWidth="150.0" text="Title" fx:id="titleColumn" />
            <TableColumn prefWidth="150.0" text="Description" fx:id="descriptionColumn"/>
            <TableColumn prefWidth="150.0" text="Status" fx:id="statusColumn"/>
            <TableColumn prefWidth="150.0" text="submitee"  fx:id="submiteeColumn"/>
        </columns>
    </TableView>
</AnchorPane>